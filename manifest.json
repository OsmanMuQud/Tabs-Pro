{
  "manifest_version": 3,
  "name": "Tabs Pro",
  "description": "This extension helps you to manage your tabs and be more productive.",
  "permissions": ["tabs", "tabGroups", "storage"],
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "css": ["popup.css"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
